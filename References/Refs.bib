@Article{Yamaguchi2012,
  Title                    = {On the relative benefits of a multi-centre grand ensemble for tropical cyclone track prediction in the western North Pacific},
  Author                   = {Yamaguchi, Munehiko and Nakazawa, Tetsuo and Hoshino, Shunsuke},
  Journal                  = {Quarterly Journal of the Royal Meteorological Society},
  Year                     = {2012},
  Number                   = {669},
  Pages                    = {2019--2029},
  Volume                   = {138},

  Application              = {rare event},
  Data                     = {TIGGE},
  Model                    = {Multi-model},
  Owner                    = {clair},
  Publisher                = {Wiley Online Library},
  Purpose                  = {uncertainty quantification},
  Review                   = {Comparison of multi-model ensemble prediction MCGE (taking % of members forecasting event to be probability of occurrence) vs single-model ensemble SME..
No post-processing calibration: looking at cyclone strike probability.

Results at 0-5 days:
 - each individual model had +ve BSS: the probabilistic predictions by each SME are statistically useful (compared with climatological predictions)
 - BSS of MCGE also +ve, but smaller than the best SME (which was ECMWF).
 - decomposition of BSS into reliability, resolution & uncertainty gives more nuanced picture. MCGE has reduced sharpness (therefore reduced resolution) but greater reliability, especially in the high-probability range.
 - MCGE also gives reduction in number of missed events.

Results at 3-5 days:
 - only one SME had positive Brier score (ECMWF). MCGEs had larger +ve skill score than this.
 - all SMEs were overconfident (forecast probability larger than observed frequency)

There remain some cases of MCGE bias, beccause all models shared the same bias.},
  Status                   = {read},
  Timestamp                = {2016.10.26}
}

